{
  "$schema": "https://esm.sh/wrangler/config-schema.json",
  "name": "sandbox-demo",
  "keep_vars": true,
  "workers_dev": true,
  "preview_urls": true,
  "compatibility_date": "2025-10-25",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "./dist",
    "html_handling": "auto-trailing-slash"
  },
  "observability": {
    "enabled": true
  }
}
